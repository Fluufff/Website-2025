{
  "extends": ["astro/tsconfigs/strictest", "@vue/tsconfig/tsconfig.json"],
  "include": [".astro/types.d.ts", ".astro/content.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "target": "ES2017",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["unplugin-icons/types/vue", "deno"]
  }
}
